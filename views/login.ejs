<body class="bg-dark">
	
	<!-- PRELOADER -->
	<div id="preloader">
		<div class="loader_line"></div>
	</div>
	<!-- /PRELOADER -->
	
<!-- WRAPPER ALL -->
<div class="shane_tm_all_wrap" data-magic-cursor="" data-color="black">
	
    <div class="shane_tm_modalbox_news">
		<div class="box_inner">
			<div class="close">
				<a href="#"><img class="svg" src="img/svg/cancel.svg" alt="" /></a>
			</div>
			<div class="description_wrap scrollable">
			</div>
		</div>
	</div>
	<!-- /MODALBOX NEWS -->
	
	
    <!-- login -->
    <% var error  %> 

    <div class="login" >
        <div  class="container text-light d-flex justify-content-center align-items-center flex-direction-column " style="height:100vh;width:40vw;" >
            
                <!-- Login Form s-->
                <form method="POST" action="/login" >
                    <!-- action="/login" -->
                        <h4 class="login-title h1" style="color:rgb(199, 187, 17)">Welcome  Costumer</h4>
                        
    
                        <% if(error) {%> 
                            <div class="alert my-2 alert-danger">
                                <%= error  %> 
                            </div>
                        <% } %> 
    
                        <div class="row">
                            
                             
                            
                                <label class="my-2 fs-4"  for="email">Email Address 
                                </label>
                                <input required class="mb-0 form-control col-10" type="email"name="email" id="email" placeholder="Email Address">
                            
                            
                                <label class="my-2 mt-4 fs-4"  for="password">Password</label>
                                <input required class="mb-0 form-control col-10"name="password" type="password" id="password" placeholder="Enter your password">
                            
                         
                            
                                <button  class="btn btn-outline-warning mt-4 fw-800 col-12">Login</button>
                                
                                <p class="mt-3">Akkautingiz yo'qmi? Unda <a href="/register"> ro'yhatdan o'ting</a></p>
                                
                </form>
            </div>
        </div>
    </div>
    


      
	<!-- CURSOR -->
	<div class="mouse-cursor cursor-outer"></div>
    <div class="mouse-cursor cursor-inner"></div>
	<!-- /CURSOR -->
	
</div>



	
